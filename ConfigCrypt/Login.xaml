        <PasswordBox Height="23" HorizontalAlignment="Left" Name="txtPassword" VerticalAlignment="Top" Width="189" Margin="77,47,0,0" PasswordChanged="txtPassword_PasswordChanged" />
        <Label Content="Password:" Height="28" HorizontalAlignment="Left" Name="label2" VerticalAlignment="Top" Width="65" Margin="12,47,0,0" />
        <Button Background="#3193CF"  Content="Login" Height="23" Name="btnLogin" VerticalAlignment="Top" IsDefault="True" Click="btnLogin_Click" Margin="97,106,106,0" IsEnabled="False" />
    </Grid>
</Window>
